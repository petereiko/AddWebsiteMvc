                transition: color 0.3s ease;
            }

        .member-info p {
            margin-top: 10px;
            font-size: 14px;
            color: #555;
        }

        /*--------------------------------------------------------------
                        # Partners Section CSS - Fixed and Enhanced
                        --------------------------------------------------------------*/

        .partners {
            display: flex;
            justify-content: center;
            align-items: center;
            padding: 0 20px;
        }

            .partners > div {
                text-align: center;
                width: 100%;
                max-width: 1000px;
            }

            .partners h4 {
                font-size: 28px;
                margin: 0 0 35px 0;
                padding: 0;
                line-height: 1.2;
                font-weight: 700;
                color: #198754;
                position: relative;
                display: inline-block;
            }

                .partners h4::after {
                    content: "";
                    position: absolute;
                    bottom: -8px;
                    left: 50%;
                    transform: translateX(-50%);
                    width: 50px;
                    height: 3px;
                    background: linear-gradient(135deg, #198754, #28a745);
                    border-radius: 2px;
                }

        /* Static Partners Grid - Side by Side */
        .partners-grid {
            display: flex;
            justify-content: center;
            align-items: center;
            gap: 40px;
            flex-wrap: wrap;
            margin-top: 30px;
            padding: 0 20px;
        }

            .partners-grid a {
                display: flex;
                align-items: center;
                justify-content: center;
                padding: 25px 35px;
                background: rgba(255, 255, 255, 0.95);
                border-radius: 15px;
                transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
                box-shadow: 0 5px 20px rgba(25, 135, 84, 0.15);
                border: 2px solid rgba(25, 135, 84, 0.1);
                position: relative;
                overflow: hidden;
                min-height: 120px;
                flex: 0 1 auto;
            }

                .partners-grid a::before {
                    content: "";
                    position: absolute;
                    top: 0;
                    left: -100%;
                    width: 100%;
                    height: 100%;
                    background: linear-gradient(90deg, transparent, rgba(25, 135, 84, 0.1), transparent);
                    transition: left 0.6s ease;
                }

                .partners-grid a:hover::before {
                    left: 100%;
                }

                .partners-grid a:hover {
                    transform: translateY(-5px) scale(1.05);
                    box-shadow: 0 10px 30px rgba(25, 135, 84, 0.25);
                    border-color: #198754;
                    text-decoration: none;
                }

            .partners-grid img {
                max-width: 180px;
                max-height: 90px;
                width: auto;
                height: auto;
                display: block;
                transition: all 0.3s ease;
                filter: grayscale(0%) brightness(1);
            }

            .partners-grid a:hover img {
                filter: grayscale(0%) brightness(1.1);
                transform: scale(1.08);
            }

        /* Carousel Partners - Animated Sliding */
        .partners-carousel {
            position: relative;
            overflow: hidden;
            padding: 20px 0;
            margin-top: 30px;
        }

        .partners-carousel-track {
            display: flex;
            gap: 40px;
            animation: scroll-partners 5s linear infinite;
            width: max-content;
        }

        .partners-carousel:hover .partners-carousel-track {
            animation-play-state: paused;
        }

        .partners-carousel a {
            display: flex;
            align-items: center;
            justify-content: center;
            padding: 25px 35px;
            background: rgba(255, 255, 255, 0.95);
            border-radius: 15px;
            transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
            box-shadow: 0 5px 20px rgba(25, 135, 84, 0.15);
            border: 2px solid rgba(25, 135, 84, 0.1);
            position: relative;
            overflow: hidden;
            min-height: 120px;
            flex-shrink: 0;
        }

            .partners-carousel a::before {
                content: "";
                position: absolute;
                top: 0;
                left: -100%;
                width: 100%;
                height: 100%;
                background: linear-gradient(90deg, transparent, rgba(25, 135, 84, 0.1), transparent);
                transition: left 0.6s ease;
            }

            .partners-carousel a:hover::before {
                left: 100%;
            }

            .partners-carousel a:hover {
                transform: translateY(-5px) scale(1.05);
                box-shadow: 0 10px 30px rgba(25, 135, 84, 0.25);
                border-color: #198754;
                text-decoration: none;
            }

        .partners-carousel img {
            max-width: 180px;
            max-height: 90px;
            width: auto;
            height: auto;
            display: block;
            transition: all 0.3s ease;
            filter: grayscale(0%) brightness(1);
        }

        .partners-carousel a:hover img {
            filter: grayscale(0%) brightness(1.1);
            transform: scale(1.08);
        }

        /* Carousel Animation */
        @@keyframes scroll-partners {
            0% {
                transform: translateX(0);
            }

            100% {
                transform: translateX(-50%);
            }
        }

        /* Gradient Fade Effect on Carousel Edges */
        .partners-carousel::before,
        .partners-carousel::after {
            content: "";
            position: absolute;
            top: 0;
            width: 100px;
            height: 100%;
            z-index: 2;
            pointer-events: none;
        }

        .partners-carousel::before {
            left: 0;
            background: linear-gradient(to right, rgba(255, 255, 255, 1), rgba(255, 255, 255, 0));
        }

        .partners-carousel::after {
            right: 0;
            background: linear-gradient(to left, rgba(255, 255, 255, 1), rgba(255, 255, 255, 0));
        }

        /* Responsive adjustments */
        @@media (max-width: 768px) {
            .partners h4 {
                font-size: 24px;
                margin-bottom: 25px;
            }

            .partners-grid {
                gap: 25px;
                padding: 0 10px;
            }

                .partners-grid a,
                .partners-carousel a {
                    padding: 20px 25px;
                    min-height: 100px;
                }

                .partners-grid img,
                .partners-carousel img {
                    max-width: 140px;
                    max-height: 70px;
                }

            .partners-carousel-track {
                gap: 30px;
            }
        }

        @@media (max-width: 480px) {
            .partners h4 {
                font-size: 22px;
            }

            .partners-grid {
                flex-direction: column;
                gap: 20px;
            }

                .partners-grid a,
